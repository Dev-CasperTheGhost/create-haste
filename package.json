{
  "name": "pastebin-api",
  "version": "2.0.3",
  "description": "A very simple pastebin npm package to interact with the pastebin api!",
  "license": "MIT",
  "author": {
    "name": "Dev-CasperTheGhost",
    "url": "https://caspertheghost.me"
  },
  "type": "module",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=12.17"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist/ && tsc",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,yml}\" --ignore-path .gitignore"
  },
  "keywords": [
    "pastebin",
    "hastebin",
    "paste",
    "pastebin-api"
  ],
  "dependencies": {
    "fast-xml-parser": "^3.19.0",
    "node-fetch": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dev-CasperTheGhost/pastebin-api"
  },
  "bugs": {
    "url": "https://github.com/Dev-CasperTheGhost/pastebin-api/issues"
  },
  "homepage": "https://github.com/Dev-CasperTheGhost/pastebin-api#readme",
  "devDependencies": {
    "@casper124578/eslint-config": "^0.0.4",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^15.0.3",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": false,
    "printWidth": 100,
    "tabWidth": 2
  }
}
