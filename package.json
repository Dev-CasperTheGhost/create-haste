{
  "name": "pastebin-api",
  "version": "1.0.6",
  "description": "A very simple pastebin npm package to interact with the pastebin api!",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist/ && tsc",
    "lint:check": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,yml}\" --ignore-path .gitignore"
  },
  "keywords": [
    "pastebin",
    "hastebin",
    "paste",
    "pastebin-api"
  ],
  "author": "Dev-CasperTheGhost",
  "license": "MIT",
  "dependencies": {
    "fast-xml-parser": "^3.19.0",
    "node-fetch": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dev-CasperTheGhost/pastebin-api"
  },
  "bugs": {
    "url": "https://github.com/Dev-CasperTheGhost/pastebin-api/issues"
  },
  "homepage": "https://github.com/Dev-CasperTheGhost/pastebin-api#readme",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^15.0.1",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  }
}
